{
  "name": "@bubbles/components",
  "version": "1.0.0",
  "description": "The Bubbles Design System is Leemonade's open-source design system for products and experiences.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leemonade/bubbles.git",
    "directory": "packages/components"
  },
  "bugs": "https://github.com/leemonade/bubbles/issues",
  "files": [
    "lib/**/*",
    "es/**/*",
    "umd/**/*"
  ],
  "keywords": [
    "react",
    "bubbles",
    "bubbles-components",
    "leemons",
    "bubbles-design-system",
    "components"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && node scripts/build.js && rollup -c",
    "start": "yarn storybook",
    "build-storybook": "cross-env NODE_ENV=production build-storybook -s ./.storybook/assets",
    "storybook": "rimraf node_modules/.cache/storybook && start-storybook -p 9000 -s ./.storybook/assets",
    "clean": "rimraf es lib umd storybook-static build"
  },
  "dependencies": {
    "@bubbles/styles": "1.0.0",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
