    "lodash": "^4.17.21",
    "luxon": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-big-calendar": "^0.38.1",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
