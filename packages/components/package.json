    "@storybook/addon-links": "^6.3.12",
    "@storybook/addons": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/source-loader": "^6.3.12",
    "autoprefixer": "^10.3.7",
    "babel-plugin-dev-expression": "^0.2.2",
    "cross-env": "^7.0.3",
    "in-publish": "^2.0.1",
    "mini-css-extract-plugin": "^2.4.2",
    "npm-which": "^3.0.1",
    "postcss-custom-properties": "^12.0.0",
